{
  "name": "fresher-client",
  "version": "0.0.1",
  "description": "fresherClient",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.js --open --host 192.168.0.5",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "server": "http-server -g",
    "test": "mocha  --require  babel-register",
    "tests": "mocha  --require babel-register --recursive './src/js/**/__tests__/*.*'",
    "eslint": "eslint './src/js/**/*.*'",
    "stylelint": "stylelint './src/css/*.*'",
    "css": "node-sass ./src/css/style.scss ./public/css/style.css",
    "watch-css": "node-sass -w ./src/css/style.scss ./public/css/style.css",
    "webpack": "webpack --config=webpack.dev.js",
    "build:dev": "node-sass ./src/css/style.scss ./public/css/style.css && webpack --config=webpack.dev.js",
    "build:test": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.test.js",
    "build:staging": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.staging.js",
    "build:production": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.prod.js"
  },
  "author": "@fresher",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.24",
    "jwt-simple": "^0.5.6",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "rc-pagination": "^2.2.0",
    "react": "^16.9.0",
    "react-datepicker": "^2.8.0",
    "react-dom": "^16.9.0",
    "react-notifications": "^1.6.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-switch": "^5.0.1",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-notes": "^5.0.5",
    "@storybook/addon-viewport": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jsdom": "^14.0.0",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.1"
  }
}
